://localhost:3030/api/v1/auth/login', //use login endpoint\r\n        {email : e.email, password: e.password}) // object of loginemail & loginpassword \r\n        .then((response) =>{\r\n        \r\n            //  setTest(false)\r\n            if(response.data.auth === true)  //if login is successfull\r\n            {   \r\n                isLogin()    //login status is changed to true\r\n                setStatus(response.data.auth)\r\n                localStorage.setItem('token',response.data.token)  //token saved to local machine\r\n                //reidrected to homepage\r\n            \r\n                //this id will be used using adding website\r\n                history.push({\r\n                    pathname:'/home',\r\n                    state:{\r\n                        isAuth : response.data.auth\r\n                    },\r\n                })\r\n            }\r\n            else{\r\n                //redirect to login page when unsuccessfull\r\n                return(<Redirect to='/'/>)\r\n            }\r\n        })\r\n        .catch(err =>{\r\n            \r\n            if(err.response.data.auth === false){\r\n              setShowPops(1)\r\n            }\r\n            else if(err.response.data.message !=null){\r\n              setShowPops(2)\r\n            }                             \r\n        })\r\n    }\r\n   \r\n    const resetShowPops = ()=>{ \r\n        setShowPops(0) \r\n    }\r\n   \r\n    return ( \r\n    \r\n           <Paper  className={classes.paper} >\r\n             \r\n              { \r\n                showPops === 1 ? <Pops resetShowPops = {resetShowPops} type={'error'} text={'Invalid Password'}/> \r\n              : showPops === 2 ? <Pops resetShowPops = {resetShowPops} type={'error'} text={'No user Found'}/> \r\n              : null\r\n              }\r\n              <Grid align=\"center\" style={{padding:'30px 0px 0px 0px'}}>\r\n                 <Avatar className={classes.color}><LockIcon/></Avatar>\r\n                 <h3>Sign In</h3>\r\n              </Grid>\r\n              \r\n            {/*using formik & yup Validation*/}\r\n              <Formik \r\n                initialValues={{\r\n                    email:\"\",\r\n                    password:\"\"\r\n                }}\r\n                validationSchema={UserSchemaLogin}\r\n                onSubmit={submitHandler}\r\n                >\r\n                   \r\n                {/* signin form  */}\r\n                    <Form >\r\n                        {/* changing the state of LoginEmail whenever user enters a letter */}\r\n                        <Grid style={{padding:'10px 40px'}}>\r\n                                <Field as={TextField}\r\n                                label=\"E-mail\"\r\n                                name=\"email\"\r\n                                placeholder=\"Enter your Email\"\r\n                                fullWidth required\r\n                                helperText={<ErrorMessage name=\"email\"/>}\r\n                            />\r\n                        </Grid>\r\n                        {/* changing the state of LoginPassword whenever user enters a letter */}\r\n                        <Grid style={{padding:'10px 40px'}}>\r\n                                <Field as={TextField} \r\n                                label=\"Password\"\r\n                                name=\"password\"\r\n                                placeholder=\"Enter the Password\"\r\n                                type=\"password\"\r\n                                fullWidth required\r\n                                helperText={<ErrorMessage name=\"password\"/>}\r\n                            />\r\n                            </Grid>\r\n \r\n                            {/* sign in button */}\r\n                            <Grid style={{padding:'30px 40px'}}  align=\"center\">\r\n                                <Button type=\"submit\" fullWidth style={{backgroundColor:'#2ecc71',color:'white'}}>\r\n                                    Sign Up\r\n                                </Button>\r\n                            </Grid>\r\n                        </Form>\r\n                 </Formik>  \r\n                 {/* reset password implemented later */}\r\n                <Grid style={{padding:'00px 0px'}}  align=\"center\">\r\n                  <Typography className={classes.root}>\r\n                        <Link href=\"#\" >\r\n                            Forgot Password?\r\n                        </Link>\r\n                  </Typography>\r\n                </Grid>\r\n              \r\n           </Paper>\r\n\r\n    )\r\n}\r\n\r\nexport default Login","C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\home\\Navbar.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\home\\logs\\Chart.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\user\\Validation.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\home\\logs\\LogsTable.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\home\\logs\\Tables.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\dialog\\Dialog.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\dialog\\Pops.js",[],"C:\\Users\\ASHISH KUMAR\\Desktop\\Pip-Bot-Frontend\\src\\components\\dialog\\LineBar.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":71,"column":16,"nodeType":"92","messageId":"93","endLine":71,"endColumn":23},{"ruleId":"90","severity":1,"message":"94","line":210,"column":11,"nodeType":"92","messageId":"93","endLine":210,"endColumn":20},{"ruleId":"95","severity":1,"message":"96","line":233,"column":43,"nodeType":"97","messageId":"98","endLine":233,"endColumn":44},{"ruleId":"90","severity":1,"message":"99","line":28,"column":12,"nodeType":"92","messageId":"93","endLine":28,"endColumn":18},"no